{
  "name": "comet-actions-models",
  "version": "0.0.1",
  "license": "UNLICENSED",
  "description": "Nx monorepo that contains shared models libraries for the Comet Actions Composite API and UI",
  "repository": {
    "type": "git",
    "url": "https://github.cloud.capitalone.com/comet/comet-actions-models.git"
  },
  "scripts": {
    "nx": "nx",
    "help": "nx help",
    "update": "nx migrate latest",
    "workspace-generator": "nx workspace-generator",
    "build:nx": "npx run-p -n lint test build",
    "lint": "nx workspace-lint && nx affected:lint",
    "test": "nx affected:test --runInBand",
    "lint:failed": "npm run lint -- --only-failed",
    "test:failed": "npm run test -- --only-failed",
    "build": "nx affected:build",
    "docs:dev": "npx vuepress dev docs",
    "docs:build": "npx vuepress build docs",
    "docs:publish": "npx gh-pages -d docs/.vuepress/dist",
    "docs:pipeline": "./tools/scripts/publish-docs.sh",
    "package": "nx affected --target=package",
    "publish": "nx affected --target=publish",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "dep-graph": "nx dep-graph",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:dep-graph": "nx affected:dep-graph",
    "build:pipeline:nx": "npx run-p -n lint:pipeline test:pipeline build:pipeline",
    "lint:pipeline": "npm run lint -- --base=origin/master~1",
    "test:pipeline": "npm run test -- --base=origin/master~1 && npm run test-coverage:merge",
    "test-coverage:merge": "mkdir -p coverage && touch coverage/lcov.info && lcov-result-merger 'coverage/**/lcov.info' 'coverage/lcov.info'",
    "build:pipeline": "npm run build -- --base=origin/master~1 && npm run package -- --base=origin/master~1",
    "publish:libs": "npm run publish -- --base=origin/master~1",
    "version:patch": "nx affected --target=version --type=patch",
    "version:minor": "nx affected --target=version --type=minor",
    "version:major": "nx affected --target=version --type=major"
  },
  "private": true,
  "devDependencies": {
    "@nrwl/cli": "12.9.0",
    "@nrwl/devkit": "12.9.0",
    "@nrwl/eslint-plugin-nx": "12.9.0",
    "@nrwl/jest": "12.9.0",
    "@nrwl/linter": "12.9.0",
    "@nrwl/node": "12.9.0",
    "@nrwl/tao": "12.9.0",
    "@nrwl/workspace": "12.9.0",
    "@types/jest": "26.0.8",
    "@types/node": "14.14.33",
    "@typescript-eslint/eslint-plugin": "4.28.5",
    "@typescript-eslint/parser": "4.28.5",
    "@vuepress/shared-utils": "^1.8.2",
    "dotenv": "10.0.0",
    "eslint": "7.22.0",
    "eslint-config-prettier": "8.1.0",
    "jest": "27.0.3",
    "lcov-result-merger": "^3.1.0",
    "prettier": "2.3.2",
    "ts-jest": "27.0.3",
    "ts-node": "~9.1.1",
    "typescript": "4.3.5",
    "vuepress": "^1.8.2",
    "vuepress-plugin-mermaidjs": "^1.8.1",
    "vuepress-theme-yuu": "^3.0.1"
  },
  "dependencies": {
    "@c1/omni-credit": "^3.1.0"
  },
  "engines": {
    "node": "^12",
    "npm": "^6"
  }
}
